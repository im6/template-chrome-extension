
{
  "name": "chrome extension starter",
  "short_name": "ch-ex-st",
  "description": "chrome extension starter",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [],
  "browser_action": {
    "default_title": "chrome extension starter"
  },
  "icons" : {
    "16" : "icon-16.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
    "css": [],
    "js": [
      "jquery-3.4.1.min.js",
      "content.js"
    ],
    "matches": [
      "http://www.YOUR-DOMAIN.com/*"
    ]
  }]
}